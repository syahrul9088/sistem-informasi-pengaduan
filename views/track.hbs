<main class="main-content mt-0">
    <div class="page-header align-items-start min-vh-50 pt-5 pb-11 m-3 border-radius-lg" style="background-image: url('https://raw.githubusercontent.com/creativetimofficial/public-assets/master/argon-dashboard-pro/assets/img/signup-cover.jpg'); background-position: top;">
      <span class="mask bg-gray"></span>
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-lg-5 text-center mx-auto">
            {{!-- <h6 class="text-white mb-2 mt-7">Selamat Datang Di Pengaduan Online Desa Jayamukti</h6> --}}
            <p class="text-lead text-white"></p>
          </div>
        </div>
      </div>
    </div>
    <div class="container">
      <div class="row mt-lg-n10 mt-md-n11 mt-n12 justify-content-center">
        <div class="col-xl-4 col-lg-5 col-md-7 mx-auto">
          <div class="card z-index-0 bg-gray">
            <div class="card-body">
              {{#if message}}
              <div class="alert alert-{{message.type}} alert-dismissible fade show" role="alert">
                <span class="alert-icon"><i class="ni ni-like-2"></i></span>
                <span class="alert-text text-white"><strong>{{message.status}}</strong> {{message.message}}</span>
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
              </div>
            {{/if}}
              <form role="form" action="/auth/track" method="POST">
                <div class="mb-3">
                  <input type="text" name="reportId" class="form-control" placeholder="ID Laporan" aria-label="Name" required>
                </div>
                <div class="text-center">
                  <button type="submit" class="btn btn-lg btn-primary btn-lg w-100 mt-4 mb-0">Cek Laporan</button>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
</main>

